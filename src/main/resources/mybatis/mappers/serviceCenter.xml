<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="serviceCenter">


<!-- 글쓰기 -->
<insert id="write" parameterType="com.javaex.vo.ServiceCenterVo">

<![CDATA[
insert into serviceCenter
	values(c_no.nextval,2,9,#{c_title},#{c_content},sysdate)

]]>

</insert>


<!-- 리스트 -->

<select id="list" resultType="com.javaex.vo.ServiceCenterVo">

<![CDATA[

select c_no,
       c_title,
       c_content,
       c_date
       from servicecenter


]]>





</select>





















</mapper>
